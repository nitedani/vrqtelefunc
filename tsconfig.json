{
  "compilerOptions": {
    "baseUrl": ".",
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "module": "esnext",
    "moduleResolution": "Node",
    "target": "esnext",
    "outDir": "dist",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext",
      "WebWorker"
    ],
    "types": [
      "vite/client"
    ],
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
  },
}